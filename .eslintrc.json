{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", { "endOfLine": "auto" }],
    "camelcase": ["error", { "properties": "always" }]
  },
  "env": {
    "browser": true,
    "node": true
  },

  "import/extensions": ["error", "always"]
}
